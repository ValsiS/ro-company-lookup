{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://raw.githubusercontent.com/ValsiS/ro-company-lookup/main/docs/schemas/company-simple.en.v1.json",
  "title": "CompanySimpleData (en)",
  "type": "object",
  "properties": {
    "company": {
      "type": "object",
      "properties": {
        "cui": { "type": "integer" },
        "registration_number": { "type": ["string", "null"] },
        "name": { "type": ["string", "null"] },
        "profile": { "$ref": "#/definitions/profile" }
      },
      "required": ["cui"],
      "additionalProperties": true
    },
    "address": {
      "type": "object",
      "properties": {
        "fiscal_domicile": { "$ref": "#/definitions/address" },
        "registered_office": { "$ref": "#/definitions/address" }
      },
      "additionalProperties": true
    },
    "caen_code": { "$ref": "#/definitions/caen" },
    "contact": {
      "type": "object",
      "properties": {
        "phone_numbers": { "type": "array", "items": { "type": "string" } },
        "emails": { "type": "array", "items": { "type": "string" } }
      },
      "additionalProperties": true
    },
    "legal_form": {
      "type": "object",
      "properties": {
        "current": { "$ref": "#/definitions/legal_form" },
        "history": {
          "type": "array",
          "items": { "$ref": "#/definitions/legal_form" }
        }
      },
      "additionalProperties": true
    },
    "vat_status": {
      "type": "object",
      "properties": {
        "current": { "$ref": "#/definitions/vat_status" },
        "history": {
          "type": "array",
          "items": { "$ref": "#/definitions/vat_status" }
        }
      },
      "additionalProperties": true
    },
    "vat_collection": { "$ref": "#/definitions/vat_collection" },
    "inactive_status": { "$ref": "#/definitions/inactive_status" },
    "split_vat": { "$ref": "#/definitions/split_vat" },
    "meta": {
      "type": "object",
      "properties": {
        "source": { "type": "string" },
        "queried_at": { "type": "string" },
        "queried_for_date": { "type": "string" },
        "is_stale": { "type": "boolean" },
        "cache_hit": { "type": "boolean" },
        "raw": { "type": ["object", "array", "null"] }
      },
      "required": ["source", "queried_at", "queried_for_date", "is_stale", "cache_hit"],
      "additionalProperties": true
    }
  },
  "required": [
    "company",
    "address",
    "caen_code",
    "contact",
    "legal_form",
    "vat_status",
    "meta"
  ],
  "additionalProperties": false,
  "definitions": {
    "caen": {
      "type": ["object", "null"],
      "properties": {
        "code": { "type": ["string", "null"] },
        "label": { "type": ["string", "null"] },
        "version": { "type": ["integer", "null"] }
      },
      "additionalProperties": true
    },
    "address": {
      "type": ["object", "null"],
      "properties": {
        "formatted_address": { "type": ["string", "null"] },
        "raw_address": { "type": ["string", "null"] },
        "country": { "type": ["string", "null"] },
        "county": { "type": ["string", "null"] },
        "county_code": { "type": ["string", "null"] },
        "county_auto_code": { "type": ["string", "null"] },
        "city": { "type": ["string", "null"] },
        "locality_code": { "type": ["string", "null"] },
        "street": { "type": ["string", "null"] },
        "number": { "type": ["string", "null"] },
        "postal_code": { "type": ["string", "null"] },
        "details": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "profile": {
      "type": ["object", "null"],
      "properties": {
        "registration_date": { "type": ["string", "null"] },
        "registration_status": { "type": ["string", "null"] },
        "fiscal_office": { "type": ["string", "null"] },
        "ownership_form": { "type": ["string", "null"] },
        "e_invoice_status": { "type": ["boolean", "null"] },
        "e_invoice_registration_date": { "type": ["string", "null"] },
        "iban": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "legal_form": {
      "type": ["object", "null"],
      "properties": {
        "updated_at": { "type": ["string", "null"] },
        "name": { "type": ["string", "null"] },
        "organization": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "vat_status": {
      "type": ["object", "null"],
      "properties": {
        "code": { "type": ["integer", "null"] },
        "label": { "type": ["string", "null"] },
        "vat_start_date": { "type": ["string", "null"] },
        "vat_cancellation_date": { "type": ["string", "null"] },
        "vat_cancellation_operation_date": { "type": ["string", "null"] },
        "queried_at": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "vat_collection": {
      "type": ["object", "null"],
      "properties": {
        "enabled": { "type": ["boolean", "null"] },
        "start_date": { "type": ["string", "null"] },
        "end_date": { "type": ["string", "null"] },
        "published_at": { "type": ["string", "null"] },
        "updated_at": { "type": ["string", "null"] },
        "act_type": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "inactive_status": {
      "type": ["object", "null"],
      "properties": {
        "is_inactive": { "type": ["boolean", "null"] },
        "inactivated_at": { "type": ["string", "null"] },
        "reactivated_at": { "type": ["string", "null"] },
        "published_at": { "type": ["string", "null"] },
        "removed_at": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    },
    "split_vat": {
      "type": ["object", "null"],
      "properties": {
        "enabled": { "type": ["boolean", "null"] },
        "start_date": { "type": ["string", "null"] },
        "cancelled_at": { "type": ["string", "null"] }
      },
      "additionalProperties": true
    }
  }
}
